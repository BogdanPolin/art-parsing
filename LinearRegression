library(readxl)
Index <- read_excel("Index.xlsx")
index1 = lm(P ~ 0 +Y2004 + Y2005 + Y2006 + Y2007 + Y2008 + Y2009 + Y2010 + Y2011 + Y2012 + Y2013 + Y2014 + Y2015 + Y2016 + Y2017 + Y2018 + Y2019 + Y2020)
index2 = lm(P ~ 0 +Y2004 + Y2005 + Y2006 + Y2007 + Y2008 + Y2009 + Y2010 + Y2011 + Y2012 + Y2013 + Y2014 + Y2015 + Y2016 + Y2017 + Y2018 + Y2019 + Y2020 + DD + DE + ED + EE  + EstFromDel + EstToDel + Price1 + NewSeason + LL + AL + NYNY + LNY + NYL + PL + NYP + PNY + MeMe + ML + Olympia + MP + ZZ + OHG + NYHG + LHG + LO + LP + ONY + HGHG + MNY + AP + PP + ANY + AHG + PO + NYO + PHG + HGNY + SHG + AA + MM + NYD)
write.csv(coef(index1), "Coef1.csv")
write.csv(coef(index2), "Coef2.csv")
